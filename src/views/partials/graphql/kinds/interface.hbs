<div class="doc-copy">
  {{#if description}}
    <div class="definition-description">
      {{md (interpolateReferences description)}}
    </div>
  {{/if}}
  {{#if fields}}
    <div class="definition-properties definition-fields">
      {{>graphql/kinds/_fields .}}
    </div>
  {{/if}}
  {{#if possibleTypes}}
    <div class="definition-properties definition-possible-types">
      <table>
        <thead>
          <tr>
            <th>{{name}} Types</th>
          </tr>
        </thead>
        <tbody>
          {{#each possibleTypes}}
            <tr>
              {{! The INTERFACE Types column }}
              <td>
                {{md (mdTypeLink . codify=true) addClass=(ternary (and isDeprecated (not @root.info.x-hideIsDeprecated)) "definition-deprecated" false)}}
              </td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  {{/if}}
</div>
<div class="doc-examples">
  {{#if example }}
    <div class="example-section">
      <h5>Example</h5>
      {{printJSON example}}
    </div>
  {{/if}}
</div>
