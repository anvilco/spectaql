<div class="doc-copy">
  {{#if description}}
    <div class="definition-description doc-copy-section">
      <h5>Description</h5>
      {{md (interpolateReferences description)}}
    </div>
  {{/if}}
  {{#if possibleTypes}}
    <div class="definition-properties doc-copy-section">
      <h5>Types</h5>
      <table>
        <thead>
          <tr>
            <th>Union Types</th>
          </tr>
        </thead>
        <tbody>
          {{#each possibleTypes}}
            <tr>
              <td>
                {{! The Union Types column }}
                {{md (mdTypeLink . codify=true) addClass=(ternary (and isDeprecated (not @root.info.x-hideIsDeprecated)) "definition-deprecated" false)}}
              </td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  {{/if}}
</div>
<div class="doc-examples">
  {{#if example }}
    <div class="example-section example-section-is-code">
      <h5>Example</h5>
      {{printJSON example}}
    </div>
  {{/if}}
</div>
