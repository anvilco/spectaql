<div id="introduction-item introduction-item-{{@index}}" data-traverse-target="introduction-item-{{@index}}">
  <h5 class="introduction-item-title">{{title}}</h5>
  <div class="doc-row">
    <div class="doc-copy">
      {{#if description}}
        {{md (interpolateReferences description)}}
      {{else if file }}
        {{md (loadTextFromFile file)}}
      {{/if}}
    </div>
    <div class="doc-examples">
    </div>
  </div>
</div>
